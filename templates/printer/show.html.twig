{% extends 'base.html.twig' %}

{% block title %}Printer{% endblock %}

{% block body %}
    <h1>Printer</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ printer.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ printer.Name }}</td>
            </tr>
            <tr>
                <th>Ip</th>
                <td>{{ printer.Ip }}</td>
            </tr>
            <tr>
                <th>SerialNumber</th>
                <td>{{ printer.SerialNumber }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('printer_index') }}">back to list</a>

    <a href="{{ path('printer_edit', {'id': printer.id}) }}">edit</a>

    {{ include('printer/_delete_form.html.twig') }}
{% endblock %}
